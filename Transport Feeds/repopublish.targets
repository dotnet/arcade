<Project ToolsVersion="12.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <ItemGroup>
      <ItemsToPush Condition="'$(Flat)'=='true'" Include="$(MSBuildThisFileDirectory)assets/*.*" />
      <PublishDependsOn Include="AfterBuild" />
    </ItemGroup>
    
    <!-- Command: dotnet msbuild repopublish.targets /t:PublishOutputLeg -->
    <Target Name="Build">
      <Message importance="high" text="Starting build..." />
    </Target>
    
    <Target Name="AfterBuild" AfterTargets="Build">
      <PropertyGroup> 
       <FeedType>Blob</FeedType>
       <FeedAccountName>https://karajascli.blob.core.windows.net/karthikfeed-123424</FeedAccountName>
       <FeedAccountKey></FeedAccountKey>
       <RelativePath>assets</RelativePath>
       <PublishFlatContainer>true</PublishFlatContainer>
      </PropertyGroup>
      <Message importance="high" text="Starting publish..." />
    </Target>
    
    <Import Project="$(MSBuildThisFileDirectory)Tools/Publish.targets" />  
</Project>
