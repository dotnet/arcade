<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <Target Name="TestCore" DependsOnTargets="GenerateVersions">
    <Error Condition="'$(VersionString)'!='$(ExpectedString)'" Text="Generated version string %22$(VersionString)%22 doesn't match expected %22$(ExpectedString)%22." />
    <Message Text="Expected output matches. Generated version string is %22$(VersionString)%22, expected was %22$(ExpectedString)%22." Importance="high" />
  </Target>

  <Import Project="$(MSBuildThisFileDirectory)..\..\src\Microsoft.DotNet.Build.Tasks.Versioning\targets\Versioning.targets"/>
  
</Project>
