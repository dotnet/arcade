<!-- Test for generation of versioning string for the "Dev" format and Semantic Versioning 1.0 support. -->

<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <VersioningTests>true</VersioningTests>
    <FormatName>dev</FormatName>

    <Major>4</Major>
    <Minor>5</Minor>
    <Patch>6</Patch>
    <Revision>0</Revision>

    <PreRelease>prev1</PreRelease>

    <IncludeDate>true</IncludeDate>
    <ShortDate></ShortDate>

    <VersionComparisonDate>2018-03-11</VersionComparisonDate>
    <VersionSeedDate>2018-05-14</VersionSeedDate>

    <IncludeSha>true</IncludeSha>
    <ShortSha>cafebabe</ShortSha>

    <Padding>5</Padding>
    <SemVerOne>true</SemVerOne>

    <ExpectedString>$(Major).$(Minor).$(Patch)-$(PreRelease)-00214-$(Revision)-$(ShortSha)</ExpectedString>
  </PropertyGroup>

  <Import Project="$(MSBuildThisFileDirectory)\TestCore.targets"/>

</Project>
