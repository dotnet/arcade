<Project>

  <PropertyGroup>
    <MicrosoftDotNetSourceBuildTasksAssembly>$(MSBuildThisFileDirectory)..\tools\net\$(MSBuildThisFileName).dll</MicrosoftDotNetSourceBuildTasksAssembly>
  </PropertyGroup>

  <UsingTask TaskName="ReadNuGetPackageInfos" AssemblyFile="$(MicrosoftDotNetSourceBuildTasksAssembly)" Runtime="NET" />
  <UsingTask TaskName="WritePackageUsageData" AssemblyFile="$(MicrosoftDotNetSourceBuildTasksAssembly)" Runtime="NET" />
  <UsingTask TaskName="WriteUsageReports" AssemblyFile="$(MicrosoftDotNetSourceBuildTasksAssembly)" Runtime="NET" />

</Project>
