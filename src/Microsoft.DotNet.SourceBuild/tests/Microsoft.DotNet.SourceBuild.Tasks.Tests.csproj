  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\tasks\Microsoft.DotNet.SourceBuild.Tasks.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Build.Tasks.Core" Version="$(MicrosoftBuildTasksCoreVersion)" />
